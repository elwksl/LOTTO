<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 맵핑될 DAO 인터페이스의 Full name 을 줍니다. -->
<mapper namespace="dev.lotto.dto.ShopVO">

	<!-- id 속성은 DAO 의 메소드와 같아야 한다. 여기의 ShopVO 는 mybatis-confg.xml 에 Alias 되어 
		있습니다. -->
	<select id="shopinfo" resultType="ShopVO">
		SELECT
			SHOP_ID
			,SHOP_NAME
			,ADDRESS
			,PHONE
		from TB_LOTTO_SHOP
		ORDER BY SHOP_ID
	</select>

</mapper>


